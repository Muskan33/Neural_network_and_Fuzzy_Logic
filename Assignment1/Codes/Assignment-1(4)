 KHANNA","photoUrl":"https://lh3.googleusercontent.com/a/default-user=s64","userId":"14503783361471552212"}}},"source":["def BGD_L2(X,y,w,Lambda,alpha,iters):\n","  cost_history = np.zeros(iters)\n","  for i in range(iters):\n","    hypothesis = sigmoid(np.dot(X,w.T))\n","    w = w*(1 - alpha*Lambda) - (alpha/len(y))*np.dot((hypothesis - y), X)\n","    cost_history[i] = cost_func(X,y,w,Lambda)\n","  return w , cost_history\n"],"execution_count":30,"outputs":[]},{"cell_type":"code","metadata":{"id":"o482NjgWR50N","executionInfo":{"status":"ok","timestamp":1634061547531,"user_tz":-330,"elapsed":53,"user":{"displayName":"MUSKAN KHANNA","photoUrl":"https://lh3.googleusercontent.com/a/default-user=s64","userId":"14503783361471552212"}}},"source":["def MBGD_L2(X,y,w,Lambda,alpha,iters,batch_size):\n","  cost_history = np.zeros(iters)\n","  for i in range(iters):\n","    rand_index = np.random.randint(len(y))\n","    ind_x = X[rand_index:rand_index + batch_size]\n","    ind_y = y[rand_index:rand_index + batch_size]\n","    w = w*(1 - alpha*Lambda) - (alpha/batch_size)*(ind_x.T.dot(sigmoid(ind_x.dot(w)) - ind_y))\n","    cost_history[i] = cost_func(ind_x,ind_y,w,Lambda)\n","  return w , cost_history"],"execution_count":31,"outputs":[]},{"cell_type":"code","metadata":{"id":"yT4hB1rcR5xb","executionInfo":{"status":"ok","timestamp":1634061547532,"user_tz":-330,"elapsed":53,"user":{"displayName":"MUSKAN KHANNA","photoUrl":"https://lh3.googleusercontent.com/a/default-user=s64","userId":"14503783361471552212"}}},"source":["def SGD_L2(X,y,w,Lambda,alpha,iters):\n","  cost_history = np.zeros(iters)\n","  for i in range(iters):\n","    rand_index = np.random.randint(len(y))\n","    ind_x = X[rand_index:rand_index+1]\n","    ind_y = y[rand_index:rand_index+1]\n","    w = w*(1 - alpha*Lambda) - alpha*(ind_x.T.dot(sigmoid(ind_x.dot(w)) - ind_y))\n","    cost_history[i] = cost_func(ind_x,ind_y,w,Lambda)\n","  return w , cost_history"],"execution_count":32,"outputs":[]},{"cell_type":"cod